{
    "name": "oneup/uploader-bundle",
    "type": "symfony-bundle",
    "description": "valums/file-uploader integration",
    "keywords": ["fileupload", "upload", "multifileupload"],
    "homepage": "http://1up.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Jim Schmid",
            "email": "js@1up.io"
        }
    ],
    
    "repositories": {
        "fine-uploader": {
            "type": "package",
            "package": {
                "name": "widen/fine-uploader",
                "version": "3.3.1",
                "source": {
                    "url": "git://github.com/Widen/fine-uploader.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    },
    
    "require": {
        "symfony/finder": ">=2.0.16,<2.3-dev",
        "symfony/filesystem": ">=2.0.16,<2.3-dev",
        "symfony/event-dispatcher": ">=2.0.16,<2.3-dev",
        "knplabs/knp-gaufrette-bundle": "0.1.*",
        "widen/fine-uploader": "3.3.*"
    },
    
    "autoload": {
        "psr-0": { "Oneup\\UploaderBundle": "" }
    },

    "target-dir": "Oneup/UploaderBundle"
}